.IFJcode18

JUMP %f1
LABEL sum_three
	PUSHFRAME
	DEFVAR LF@ret

	DEFVAR LF@p1
	MOVE LF@p1 LF@%1
	DEFVAR LF@p2
	MOVE LF@p2 LF@%2
	DEFVAR LF@p3
	MOVE LF@p3 LF@%3

	ADD LF@ret LF@p1 LF@p2
	ADD LF@ret LF@ret LF@p3

	POPFRAME
	RETURN
LABEL %f1

JUMP %f2
LABEL pow
	PUSHFRAME
	DEFVAR LF@ret	

	DEFVAR LF@p1
	MOVE LF@p1 LF@%1

	MUL LF@ret LF@p1 LF@p1
	POPFRAME 
	RETURN
LABEL %f2

	DEFVAR GF@a
	MOVE GF@a int@6
	DEFVAR GF@b
	MOVE GF@b int@7
	DEFVAR GF@c
	MOVE GF@c int@8
	
	CREATEFRAME
	DEFVAR TF@%1
	MOVE TF@%1 GF@a
	DEFVAR TF@%2
	MOVE TF@%2 GF@b
	DEFVAR TF@%3
	MOVE TF@%3 GF@c

	CALL sum_three
	DEFVAR GF@res
	MOVE GF@res TF@ret	
	DPRINT GF@res

	CREATEFRAME
	DEFVAR TF@%1
	MOVE TF@%1 GF@a
	CALL pow
	MOVE GF@res TF@ret
	DPRINT GF@res

	EXIT int@0	
